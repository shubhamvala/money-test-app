{
  "$schema": "http://json.schemastore.org/solidaritySchema",
  "requirements": {
    "Node": [
      {
        "rule": "cli",
        "binary": "node",
        "semver": ">=16.14.0"
      }
    ],
    "Watchman": [
      {
        "rule": "cli",
        "binary": "watchman",
        "error": "Please install watchman on this machine. Refer to the official Watchman installation instructions for additional help.",
        "platform": [
          "darwin",
          "linux"
        ]
      }
    ],
    "React Native": [
      {
        "rule": "cli",
        "binary": "react-native",
        "semver": ">=10.2.2"
      }
    ],
    "Package JSON": [
      {
        "rule": "file",
        "location": "./package.json"
      }
    ],
    "NPM": [
      {
        "rule": "cli",
        "binary": "npm",
        "semver": ">=8.3.1"
      }
    ],
    "TypeScript": [
      {
        "rule": "cli",
        "binary": "tsc",
        "semver": ">=4.8.4"
      }
    ],
    "Yarn": [
      {
        "rule": "cli",
        "binary": "yarn",
        "version": "--version",
        "semver": ">=1.22.19"
      }
    ],
    "CocoaPods": [
      {
        "rule": "cli",
        "binary": "pod",
        "platform": "darwin",
        "semver": ">=1.12.1"
      }
    ],
    "Android": [
      {
        "rule": "env",
        "variable": "ANDROID_HOME",
        "error": "The ANDROID_HOME environment variable must be set to your local SDK.  Refer to getting started docs for help."
      }
    ],
    "Xcode": [
      {
        "rule": "cli",
        "binary": "xcodebuild",
        "semver": ">=14.3.1",
        "platform": "darwin"
      },
      {
        "rule": "cli",
        "binary": "xcrun",
        "semver": ">=64",
        "platform": "darwin"
      }
    ]
  }
}
